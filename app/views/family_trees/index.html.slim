= react_component 'FamilyTreeDashboard', props: { \
    people: @people.map { |p| p.as_json.merge( \
      'photo_sm_url' => cl_small_image_path(p.photo_url), \
      'photo_md_url' => cl_medium_image_path(p.photo_url) \
    )}, \
    deads: @deads.map { |d| d.as_json.merge( \
      'photo_sm_url' => cl_small_image_path(d.photo_url), \
      'photo_md_url' => cl_medium_image_path(d.photo_url) \
    )}, \
    familyTree: @family_tree.as_json \
  }